{
  "name": "test3",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "test3/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist\\{projectRoot}",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/test3",
        "main": "test3/src/index.ts",
        "tsConfig": "test3/tsconfig.lib.json",
        "assets": []
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist\\{projectRoot}"
      }
    }
  }
}
