{
  "name": "test4",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "test4/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist\\{projectRoot}",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "tags": ["test"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/test4",
        "main": "test4/src/index.ts",
        "tsConfig": "test4/tsconfig.lib.json",
        "assets": ["test4/*.md"]
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist\\{projectRoot}"
      }
    }
  }
}
